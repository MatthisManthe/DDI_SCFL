{
    "device": "cuda:0",
    
    "data_dir_cityscapes": "../../datasets/Cityscapes_test",
    "data_dir_gta": "../../datasets/GTA5_train_val_test",
    "labels": "categories",
    "cache_dir": "../../cache_dir",
    "log_dir": "../../runs",
    "log_filename": null,
    
    "cache": false,
    "sort": true,
    
    "generate_partitioning": false,
