{
    "device": "cuda:0",
    
    
    "log_dir": "../../runs",
    "log_filename": null,
    
    "dataset": "../../datasets/Triple_MNIST_0134",
    "images_name": "images.npy",
    "digits": [0, 1, 3, 4],
    "invert": true,
    "flip_horizontal": false,
    "flip_vertical": false,
    "flip_90": false,
    
    "generate_partitioning": false,
