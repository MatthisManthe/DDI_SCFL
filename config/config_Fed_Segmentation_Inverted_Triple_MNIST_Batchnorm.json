    "iid": false,
    "fraction": 0.995,
    "dirichlet": 0,
    "nb_clients": 10,
    
    "max_rounds": 700,
    "save_rounds": [20, 50, 75, 100, 150, 200, 250],
    "batch_size": 16,
    "learning_rate": 0.1,
    "momentum": 0,
    
    "lr_scheduler": "exp_decay",
    "gamma": 0.9975,

    "model_path": "...",
    "nb_restart": 3
}
